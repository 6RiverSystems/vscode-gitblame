{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es2018",
        "strict": true,
        "outDir": "./out",
        "lib": [
            "es2016",
            "es2017",
        ],
        "sourceMap": true,
        "rootDir": ".",
        "baseUrl": "src",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true
    },
    "exclude": [
        "node_modules",
    ],
}
